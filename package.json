{
  "name": "FansX-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "build:test": "NODE_OPTIONS='--inspect' NODE_ENV=test next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint --fix \"**/*.{js,jsx,ts,tsx}\"",
    "push-all": "./push-all.sh",
    "type-check": "tsc --noEmit",
    "check-all": "npm run type-check && npm run lint",
    "zip": "zip -r dist.zip .next public package.json package-lock.json next.config.ts .env",
    "zip-test": "zip -r dist.zip .next public package.json package-lock.json next.config.ts .env.test .env",
    "build:clear": "rimraf .next dist.zip && yarn build",
    "build:cz": "rimraf .next dist.zip && yarn build && zip -r dist.zip .next public package.json package-lock.json next.config.ts .env",
    "build:ctz": "rimraf .next dist.zip && yarn build:test && zip -r dist.zip .next public package.json package-lock.json next.config.ts .env.test .env"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.1",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-radio-group": "^1.2.2",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-switch": "^1.1.2",
    "@radix-ui/react-toggle": "^1.1.1",
    "@radix-ui/react-toggle-group": "^1.1.1",
    "antd-mobile": "^5.38.1",
    "chart.js": "^4.4.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "dayjs": "^1.11.13",
    "embla-carousel-react": "^8.5.2",
    "emittery": "^1.1.0",
    "emoji-picker-react": "^4.12.2",
    "force": "^0.0.3",
    "hls.js": "^1.5.20",
    "html2canvas": "^1.4.1",
    "https-proxy-agent": "^5.0.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.468.0",
    "next": "15.1.1-canary.14",
    "next-intl": "^3.26.5",
    "node-fetch": "^3.3.2",
    "react": "^19.0.0",
    "react-chartjs-2": "^5.2.0",
    "react-day-picker": "^8.10.1",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "react-textarea-autosize": "^8.5.7",
    "swr": "^2.2.5",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "use-debounce": "^10.0.4",
    "use-long-press": "^3.2.0",
    "vaul": "^1.1.2",
    "video.js": "^8.22.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.13",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/video.js": "^7.3.58",
    "eslint": "^8",
    "eslint-config-next": "15.0.4",
    "eslint-import-resolver-typescript": "^4.2.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "postcss": "^8",
    "sass": "^1.83.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
